\r\n            cards[0].setDepth(sDepth);\r\n            secondDeckHeight -= 10;\r\n            sDepth++;\r\n            cards.shift();\r\n            if (!cards.length) {\r\n                clearInterval(cardInterval);\r\n            }\r\n        }, 1000);\r\n        let backButton = createBackButton(this, cardInterval);\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"baf0edb7224bb1a2f0ff250180e83c8c","cacheData":{"env":{}}}